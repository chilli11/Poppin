<div class="card">
	<div class="card-header card-header-primary">
		<h4 class="card-title">{{@vendor.organizationName}}</h4>
		{{#if (or @isSiteAdmin @isVendorAdmin)}}
			<p><a href="javascript:void(0)" class="text-dark" {{on "click" @startEdit}}>Edit</a></p>
		{{/if}}
	</div>
	<div class="card-body">
		<dl>
			<dt>Contact</dt>
			<dd>{{@vendor.organizationContactName}}</dd>
			<dt>Email</dt>
			<dd>{{@vendor.organizationContactEmail}}</dd>
		</dl>
	</div>
</div>